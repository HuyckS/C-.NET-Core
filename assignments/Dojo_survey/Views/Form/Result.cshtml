<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Result</title>
    <link rel="stylesheet" href="~/css/style.css" />
</head>
@model Dojo_survey.Models.Survey

<body>
    <h1>Submitted Info:</h1>
    <div class="form-result">
        <div class="form-info">
            <div class="form-info-left">
                <h3>Name:</h3>
                <h3>Dojo Location:</h3>
                <h3>Favorite Language:</h3>
                <h3>Comment:</h3>
            </div>
            <div class="form-info-right">
                <h3>@Model.Name</h3>
                <h3>@Model.Location</h3>
                <h3>@Model.Language</h3>
                <h3>@Model.Comment</h3>
            </div>
        </div>
        <div>
            <a href="/">Go Back</a>
        </div>
    </div>
</body>

</html>