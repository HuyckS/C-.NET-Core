                </tr>

            }
        </table>
        <table class="table table-striped">
            <tr>
                <th>Player Name</th>
                <th>Player Team</th>
            </tr>
            @foreach (var p in ViewBag.lNameCooper)
            {
                <tr>
                    <td>@p.FirstName @p.LastName</td>
                    <td>@p.CurrentTeam</td>
                </tr>

            }
        </table>
